{
    "rules": {
        "indent": [
            1,
            2
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": 1,
        "no-unreachable": 0
    },
    "env": {
        "browser": true
    },

    "globals": {
        "angular": 1
    },
    "extends": "eslint:recommended"
}
